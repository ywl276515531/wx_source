@charset "utf-8";
html,body{
	width: 100%;
	margin: 0;
	padding: 0;
	font-size: 14px;
	font-family: "微软雅黑";
	overflow-x: hidden;
}
div,p,span,li,ul,a,h1,h2,h3,h4,h5,h6,hr,img,dl,dd{
	margin:0px;
	padding: 0px;
}
li{
	list-style: none;
}
a{
	text-decoration: none;
	outline: none;
	cursor: pointer;
}
img{
	border: none;
	max-width: 100%;
}
#personnews{
	max-width: 640px;
	margin: auto;
	border: 1px solid #CACACA;
}
.common{
	width: 98%;
	margin: 10px auto;
	height: 30px;
	line-height: 30px;
	font-size: 16px;
	text-align: center;
	background-color: #fcf5e2;
}
.item_info{
	width: 98%;
	margin: 5px auto;	
	float: left;
}
.item_info b{
	height: 100%;
	float: left;
}
.self{
	height: 100%;
}

#clear{
	clear: both;
}
#edu,#other{
	height: 30px;
}
#info_in{
	height: 200px;
}
#job_want{
	height: 50px;
}


#workexp{
	width: 98%;
	padding: 5px 10px 0 10px;
	margin: auto;
	line-height: 25px;
	height: 200px;
	word-wrap: break-word;
}