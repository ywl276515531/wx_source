@charset "utf-8";
html,body{
	width: 100%;
	margin: 0;
	padding: 0;
	font-family:"微软雅黑";
	overflow-x: hidden;
	background: white;
	font-size: 12px;
}
div,p,span,li,ul,a,h1,h2,h3,h4,h5,h6,hr,img,dl,dd{
	margin:0px;
	padding: 0px;
}
li{
	list-style: none;
}
a{
	text-decoration: none;
	outline: none;
	cursor: pointer;
}
img{
	border:none;
	max-width: 100%;
}
.clearfix{
	clear: both;
}
strong{
	font-weight: normal;
}
#cont{
	max-width: 640px;
	margin:auto;
}
#cont>form{
	width: 100%;
	height: 38px;
	line-height: 38px;
	border:1px solid #00b8ee;
	z-index: 100;
}
#cont>form>.first{
	position: relative;
	float: left;
	width: 21%;
}
#cont>form>.two{
	position: relative;
	float: left;
	width: 33%;
}
#cont>form>.first>input,#cont>form>.two>input{
	width: 100%;
}
#cont>form .first>div{
	left: 0;
	position: absolute;
	position: absolute;
    	top: 42px;
    	z-index: 5;
    	background: white;
    	border: 1px solid #ccc;
    	border-radius: 4px;
    	width: 100%;
    	display: none;
}
#cont>form .two>div{
	left: 0;
	position: absolute;
	position: absolute;
    	top: 42px;
    	z-index: 5;
    	background: white;
    	border: 1px solid #ccc;
    	border-radius: 4px;
    	width: 100%;
    	display: none;
}
#cont>form>div>div>ul{
	width: 100%;
}
#cont>form>div>div>ul>li{
	height: 25px;
	line-height: 25px;
	font-size: 12px;
	width: 100%;
	text-align: center;
}
#cont>form>div>div>ul>li:hover{
	background-size: gray;
}
#cont>form input{
	width: 23%;
	height: 38px;
	color: black;
	float: left;
	text-align: center;
	line-height: 38px;
	font-size: 12px;
	cursor: pointer;
	border:none;
	 -webkit-appearance : none ;
	 border-radius: 0;
	box-sizing: border-box;
}
#cont>form .hou{
	border-left: 1px solid #00b8ee;
}
.current{
	background: #00b8ee;
}
#zulin{
	max-width: 640px;
	margin:auto;
	margin-top: 15px;
	background: white;
	min-height: 290px;
}
#zulin>ul{
	width: 96%;
	margin:auto;
	margin-top: 10px;
	height: 240px;
}
#zulin>ul>li{
	width: 49%;
	height: 240px;
	float: left;
}
#zulin>ul>li.hou{
	margin-left: 2%;
}
#zulin>ul>li>a{
	width: 100%;
	display: block;
}
#zulin>ul>li>a>img{
	width: 100%;
	height: 200px;
}
#zulin>ul>li>.tit{
	text-align: center;
	color: black;
}
#zulin>ul .adress{
	width: 100%;
}
#zulin>ul .adress>img{
	float: left;
	padding-left: 25%;
	width: 12px;
    	margin-top: 3px;
}
#zulin>ul .adress>p{
	float: left;
	color: grey;
}
@media (min-width: 480px) and (max-width:565px){
	#cont{
		width: 100%;
	}
	#zulin{
		width: 100%;
	}
	#cont>form input{
		width: 24%;
	}
	#zulin>ul>li>.tit{
		white-space: nowrap;
		text-overflow: ellipsis;
		overflow: hidden;
	}
}
@media (min-width: 420px) and (max-width:479px){
	#cont{
		width: 100%;
	}
	#zulin{
		width: 100%;
	}
	#zulin>ul{
		height: 170px;
	}
	#zulin>ul>li>.tit{
		white-space: nowrap;
		text-overflow: ellipsis;
		overflow: hidden;
	}
	#zulin>ul>li{
		height: 170px;
	}
	#zulin>ul>li>a>img{
		height: 125px;
	}
	#zulin>ul .adress>img{
		padding-left: 15%;
	}
}
